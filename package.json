    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.13.1",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.8",
    "next-themes": "^0.4.6",
    "playwright": "^1.55.0",
    "posthog-js": "^1.261.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.53.2",
    "sharp": "^0.34.3",
    "stripe": "^17.3.1",
    "svix": "^1.76.1",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.5.4",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"

    "autoprefixer": "^10.4.20",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",

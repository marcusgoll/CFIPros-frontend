# Integration Testing Environment Configuration - Production (READ-ONLY)
# Copy to .env.integration.production and fill in your production values
# WARNING: Only read-only operations should be performed in production

# Environment designation
INTEGRATION_TEST_ENV=production

# Production test credentials (read-only user)
PROD_TEST_USER=prod-readonly-test-user
PROD_TEST_PASSWORD=prod-readonly-password
PROD_API_KEY=your_production_readonly_api_key
INTEGRATION_TEST_TIMEOUT=10000

# Production Clerk configuration
PROD_CLERK_PUBLISHABLE_KEY=pk_live_your_production_publishable_key
PROD_CLERK_SECRET_KEY=sk_live_your_production_secret_key
PROD_CLERK_WEBHOOK_SECRET=whsec_your_production_webhook_secret
PROD_CLERK_TEST_USER_ID=user_prod_readonly_test_id

# Production backend API (matches OpenAPI contract)
PROD_BACKEND_API_URL=https://api.cfipros.com/api/v1
PROD_FRONTEND_URL=https://cfipros.com

# Test data configuration (read-only for production)
USE_REAL_FILES=false
ENABLE_RATE_LIMIT_TESTS=false
ENABLE_CLEANUP=false
READ_ONLY_MODE=true
# Integration Testing Environment Configuration - Staging
# Copy to .env.integration.staging and fill in your staging values

# Environment designation
INTEGRATION_TEST_ENV=staging

# Staging test credentials (replace with actual values)
STAGING_TEST_USER=staging-test-user
STAGING_TEST_PASSWORD=staging-test-password
STAGING_API_KEY=your_staging_api_key_here
INTEGRATION_TEST_TIMEOUT=30000

# Staging Clerk configuration
STAGING_CLERK_PUBLISHABLE_KEY=pk_test_your_staging_publishable_key
STAGING_CLERK_SECRET_KEY=sk_test_your_staging_secret_key
STAGING_CLERK_WEBHOOK_SECRET=whsec_your_staging_webhook_secret
STAGING_CLERK_TEST_USER_ID=user_staging_test_id
STAGING_CLERK_TEST_ORG_ID=org_staging_test_id

# Staging backend API (matches OpenAPI contract)
STAGING_BACKEND_API_URL=https://cfipros-api-staging.up.railway.app/api/v1
STAGING_FRONTEND_URL=https://staging.cfipros.com

# Test data configuration
USE_REAL_FILES=true
ENABLE_RATE_LIMIT_TESTS=true
ENABLE_CLEANUP=true